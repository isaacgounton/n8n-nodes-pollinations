{
	"name": "n8n-nodes-pollinations-v2",
	"version": "1.1.2",
	"description": "n8n community node for Pollinations.ai - Generate images, videos, text, and audio using AI",
	"license": "MIT",
	"homepage": "https://github.com/isaacgounton/n8n-nodes-pollinations",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"ai",
		"pollinations",
		"image-generation",
		"video-generation",
		"text-generation",
		"audio-generation",
		"tts",
		"llm",
		"flux",
		"openai"
	],
	"author": {
		"name": "isaacgounton",
		"email": "info@n8n.io"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/isaacgounton/n8n-nodes-pollinations.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/PollinationsApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Pollinations/Pollinations.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^25.0.9",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
