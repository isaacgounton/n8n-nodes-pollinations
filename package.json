{
	"name": "n8n-nodes-ai-media-generate",
	"version": "0.1.35",
	"description": "n8n community node for AI media generation - Generate images and videos using various AI platforms",
	"license": "MIT",
	"homepage": "https://github.com/MaskerPRC/n8n-nodes-ai-media-generate",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"ai",
		"image-generation",
		"video-generation",
		"media-generation",
		"fal",
		"genbo",
		"replicate"
	],
	"author": {
		"name": "n8n",
		"email": "info@n8n.io"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MaskerPRC/n8n-nodes-ai-media-generate.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/FalApi.credentials.js",
			"dist/credentials/GenboApi.credentials.js",
			"dist/credentials/ReplicateApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/AiMediaGenerate/AiMediaGenerateFal.node.js",
			"dist/nodes/AiMediaGenerate/AiMediaGenerateGenbo.node.js",
			"dist/nodes/AiMediaGenerate/AiMediaGenerateReplicate.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
